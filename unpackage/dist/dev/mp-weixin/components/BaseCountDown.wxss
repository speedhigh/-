.time {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.time__custom {
  margin-top: 4px;
  width: 22px;
  height: 22px;
  background-color: #FFFFFF;
  border-radius: 4px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.time__custom__item {
  color: #1A202C;
  font-size: 12px;
  text-align: center;
}
.time__doc {
  color: #FFFFFF;
  padding: 0px 4px;
}
.time__item {
  color: #606266;
  font-size: 15px;
  margin-right: 4px;
}

